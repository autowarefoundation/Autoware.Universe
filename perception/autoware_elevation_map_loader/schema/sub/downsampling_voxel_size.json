{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Elevation Map Loader Params",
  "definitions": {
    "voxel_size": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number",
          "description": "Voxel size(x) in meters",
          "default": 0.02,
          "minimum": 0.001
        },
        "y": {
          "type": "number",
          "description": "Voxel size(y) in meters",
          "default": 0.02,
          "minimum": 0.001
        },
        "z": {
          "type": "number",
          "description": "Voxel size(z) in meters",
          "default": 0.02,
          "minimum": 0.001
        }
      },
      "required": ["x", "y", "z"],
      "additionalProperties": false
    }
  }
}
