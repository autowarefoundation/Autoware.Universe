<?xml version="1.0"?>
<launch>
  <arg name="input/objects" default="objects_raw"/>
  <arg name="output/objects" default="objects_mapped"/>
  <arg name="params_path" default="$(find-pkg-share detection_class_adapter)/config/detection_class_adapter.param.yaml"/>

  <node pkg="detection_class_adapter" exec="detection_class_adapter_node" name="detection_class_adapter" output="screen">
    <remap from="~/input/objects" to="$(var input/objects)"/>
    <remap from="~/output/objects" to="$(var output/objects)"/>

    <param from="$(var params_path)"/>
  </node>
</launch>
