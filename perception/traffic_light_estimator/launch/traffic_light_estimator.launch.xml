<?xml version="1.0"?>
<launch>
  <node pkg="traffic_light_estimator" exec="traffic_light_estimater_node" name="traffic_light_estimator" output="screen">
    <remap from="~/input/vector_map" to="/map/vector_map"/>
    <remap from="~/input/route" to="/planning/mission_planning/route"/>
    <remap from="~/input/observed/traffic_signals" to="observed/traffic_signals"/>
    <remap from="~/output/traffic_signals" to="traffic_signals"/>
  </node>
</launch>
