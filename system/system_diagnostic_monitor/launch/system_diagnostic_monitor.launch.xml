<launch>
  <arg name="param_file" default="$(find-pkg-share autoware_system_diagnostic_monitor)/config/default.param.yaml"/>
  <arg name="graph_file" default="$(find-pkg-share autoware_system_diagnostic_monitor)/config/autoware-main.yaml"/>
  <include file="$(find-pkg-share autoware_system_diagnostic_monitor)/launch/system_diagnostic_monitor.launch.xml">
    <arg name="param_file" value="$(var param_file)"/>
    <arg name="graph_file" value="$(var graph_file)"/>
  </include>
  <node pkg="autoware_system_diagnostic_monitor" exec="autoware_system_diagnostic_monitor_node"/>
</launch>
