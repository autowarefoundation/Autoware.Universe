<launch>
  <node pkg="default_ad_api_helpers" exec="initial_pose_adaptor" name="initial_pose_adaptor">
    <param from="$(find-pkg-share default_ad_api_helpers)/config/initial_pose.param.yaml"/>
    <remap from="initialpose" to="/initialpose"/>
    <remap from="fit_map_height" to="/localization/util/fit_map_height"/>
  </node>

  <node pkg="default_ad_api_helpers" exec="automatic_pose_initializer" name="automatic_pose_initializer"/>
</launch>
