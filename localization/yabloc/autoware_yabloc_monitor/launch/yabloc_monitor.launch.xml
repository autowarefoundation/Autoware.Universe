<launch>
  <arg name="param_file" default="$(find-pkg-share autoware_yabloc_monitor)/config/yabloc_monitor.param.yaml"/>

  <node pkg="autoware_yabloc_monitor" exec="autoware_yabloc_monitor_node" output="both">
    <param from="$(var param_file)"/>
    <remap from="~/input/yabloc_pose" to="/localization/pose_estimator/yabloc/pf/pose"/>
  </node>
</launch>
