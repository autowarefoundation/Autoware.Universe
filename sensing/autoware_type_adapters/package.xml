  <version>0.1.0</version>
  <description>Type Adaptation implementation for image messages</description>
  <maintainer email="yxliuzju@qq.com">Yuxuan Liu</maintainer>
  <license>Apache License 2.0</license>
  <author email="clalancette@openrobotics.org">Chris Lalancette</author>
