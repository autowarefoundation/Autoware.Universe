<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>carla_pointcloud_interface</name>
  <version>1.0.0</version>
  <description>Convert CARLA 4D point cloud message to Autoware XYZIRADT cloud message</description>
  <maintainer email="mradityagio@gmail.com">Muhammad Raditya Giovanni</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>autoware_cmake</buildtool_depend>

  <depend>libpcl-all-dev</depend>
  <depend>pcl_ros</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>sensor_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
