{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parameters for point cloud map loader Node",
  "type": "object",
  "definitions": {
    "pointcloud_map_loader": {
      "type": "object",
      "properties": {
        "enable_whole_load": {
          "type": "boolean",
          "description": "Enable loading all PCD maps",
          "default": true
        },
        "enable_downsampled_whole_load": {
          "type": "boolean",
          "description": "Enable downsampling all loaded PCDs",
          "default": false
        },
        "enable_partial_load": {
          "type": "boolean",
          "description": "Enable loading partial PCD maps",
          "default": true
        },
        "enable_selected_load": {
          "type": "boolean",
          "description": "Enable loading selected PCD maps",
          "default": false
        },
        "leaf_size": {
          "type": "number",
          "description": "Resolution of the voxel grid used for downsampling all loaded PCDs",
          "default": 3.0
        }
      },
      "required": [
          "enable_whole_load",
          "enable_downsampled_whole_load",
          "enable_partial_load",
          "enable_selected_load",
          "leaf_size"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "/**": {
      "type": "object",
      "properties": {
        "ros__parameters": {
          "$ref": "#/definitions/pointcloud_map_loader"
        }
      },
      "required": ["ros__parameters"],
      "additionalProperties": false
    }
  },
  "required": ["/**"],
  "additionalProperties": false
}
  